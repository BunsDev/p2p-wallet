{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react",
    "linaria/babel"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "constants": "./src/constants",
          "config": "./src/config",
          "utils": "./src/utils",
          "store": "./src/store",
          "api": "./src/api",
          "features": "./src/features",
          "pages": "./src/pages",
          "components": "./src/components"
        }
      }
    ],
    "@babel/transform-runtime"
  ]
}
